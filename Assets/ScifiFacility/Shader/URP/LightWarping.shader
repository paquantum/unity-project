				#endif

				half dotResult69 = dot( WorldNormal , _MainLightPosition.xyz );
				half temp_output_74_0 = ( _LightWarp * 0.5 );
				half temp_output_79_0 = max( 0.0 , ( ( dotResult69 * ( 1.0 - temp_output_74_0 ) ) + temp_output_74_0 ) );
				half4 temp_cast_1 = (temp_output_79_0).xxxx;
				half2 appendResult127 = (half2(temp_output_79_0 , 0.0));
				float2 uv_Flakers = IN.ase_texcoord7.xy * _Flakers_ST.xy + _Flakers_ST.zw;
				half2 uv0_Flakers = IN.ase_texcoord7.xy * _Flakers_ST.xy + _Flakers_ST.zw;
				half4 blendOpSrc136 = tex2D( _Flakers, uv_Flakers );
				half4 blendOpDest136 = tex2D( _Flakers, ( uv0_Flakers * float2( 10,10 ) ) );
				half2 temp_output_137_0 = (( saturate( (( blendOpDest136 > 0.5 ) ? ( 1.0 - 2.0 * ( 1.0 - blendOpDest136 ) * ( 1.0 - blendOpSrc136 ) ) : ( 2.0 * blendOpDest136 * blendOpSrc136 ) ) ))).rg;
				half2 blendOpSrc132 = appendResult127;
				half2 blendOpDest132 = temp_output_137_0;
				half2 lerpResult130 = lerp( appendResult127 , ( saturate( (( blendOpDest132 > 0.5 ) ? ( 1.0 - 2.0 * ( 1.0 - blendOpDest132 ) * ( 1.0 - blendOpSrc132 ) ) : ( 2.0 * blendOpDest132 * blendOpSrc132 ) ) )) , ( _AddFlakers * 1.5 ));
				half4 lerpResult148 = lerp( temp_cast_1 , tex2D( _ColorRump, lerpResult130 ) , _UseColorRamp);
				
				float3 Albedo = _Color.rgb;
				float3 Normal = float3(0, 0, 1);
				float3 Emission = max( float4( 0,0,0,0 ) , ( lerpResult148 * _LightWarpColor ) ).rgb;
				float3 Specular = 0.5;
				float Metallic = 0;
				float Smoothness = 0.5;
				float Occlusion = 1;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float3 BakedGI = 0;
				float3 RefractionColor = 1;
				float RefractionIndex = 1;
				
				#ifdef _ALPHATEST_ON
					clip(Alpha - AlphaClipThreshold);
				#endif

				InputData inputData;
				inputData.positionWS = WorldPosition;
				inputData.viewDirectionWS = WorldViewDirection;
				inputData.shadowCoord = ShadowCoords;

				#ifdef _NORMALMAP
					inputData.normalWS = normalize(TransformTangentToWorld(Normal, half3x3( WorldTangent, WorldBiTangent, WorldNormal )));
				#else
					#if !SHADER_HINT_NICE_QUALITY
						inputData.normalWS = WorldNormal;
					#else
						inputData.normalWS = normalize( WorldNormal );
					#endif
				#endif

				#ifdef ASE_FOG
					inputData.fogCoord = IN.fogFactorAndVertexLight.x;
				#endif

				inputData.vertexLighting = IN.fogFactorAndVertexLight.yzw;
				inputData.bakedGI = SAMPLE_GI( IN.lightmapUVOrVertexSH.xy, IN.lightmapUVOrVertexSH.xyz, inputData.normalWS );
				#ifdef _ASE_BAKEDGI
					inputData.bakedGI = BakedGI;
				#endif
				half4 color = UniversalFragmentPBR(
					inputData, 
					Albedo, 
					Metallic, 
					Specular, 
					Smoothness, 
					Occlusion, 
					Emission, 
					Alpha);

				#ifdef _REFRACTION_ASE
					float4 projScreenPos = ScreenPos / ScreenPos.w;
					float3 refractionOffset = ( RefractionIndex - 1.0 ) * mul( UNITY_MATRIX_V, WorldNormal ).xyz * ( 1.0 / ( ScreenPos.z + 1.0 ) ) * ( 1.0 - dot( WorldNormal, WorldViewDirection ) );
					float2 cameraRefraction = float2( refractionOffset.x, -( refractionOffset.y * _ProjectionParams.x ) );
